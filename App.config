            </setting>
            <setting name="SqlAuth" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="OutPath" serializeAs="String">
                <value />
            </setting>
        </Esha.Admin.Tools.Properties.Settings>
    </userSettings>
